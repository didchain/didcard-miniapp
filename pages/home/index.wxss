/* pages/home/index.wxss */
.home-header-bg {
  width: 100%;
  height: 446rpx;
  background: linear-gradient(360deg, #fbe695 0%, #f5c95c 100%);
  border-radius: 0px 0px 200rpx 200rpx;
}
.home-header .inner-box {
  width: 100%;
  display: flex;
  flex-direction: column;
  -ms-flex-direction: column;
  padding: 12rpx 24rpx;
}

.home-header .inner-box .app-title {
  font-size: 48rpx;
  font-weight: 500;
  color: #0c123d;
  line-height: 60rpx;
}

.home-header .sub-title {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
}

.home-header .inner-box .did-text {
  font-size: 30rpx;
  font-weight: 500;
  color: #0c123d;
  line-height: 40rpx;
  word-break: break-all;
  margin-right: 12px;
}

.home-header .inner-box view:not(:last-child) {
  padding-bottom: 8px;
}

/** Body begin */
.vp-main {
  padding: 6rpx 24rpx;
}

/** id card */
.v-did-card {
  height: 128px;
  background: linear-gradient(225deg, #f6c330 0%, #f5cd73 100%);
  box-shadow: 0px 2px 6px 2px rgba(12, 18, 61, 0.06);
}

.v-did-card--body {
  display: flex;
  flex: 1;
  flex-direction: row;
  justify-content: space-between;
  justify-items: center;
  padding: 12rpx auto;
}

.v-did-card--body > .v-didtext-col {
  justify-self: start;
  padding-top: 30rpx;
  flex: 1 1;
}

.v-didtext-col .id-text {
  flex: 0 1 auto;
  padding: 4rpx 0rpx;
  font-size: 24rpx;
  font-weight: 500;
  word-break: break-all;
}
.id-text {
  padding-right: 16rpx;
}

.v-qrcode-wrap {
  min-height: 256rpx;
  display: flex;
  flex-direction: column;
  flex: 0 1;
  justify-items: center;
  justify-content: center;
}

.v-qrcode-wrap .v-qrcode-box {
  display: flex;
  flex-direction: column;
  justify-self: center;
  background: #fff;
  padding: 4px;
  border-radius: 2px;
  border: 1px solid #0c123d;
  justify-items: center;
  justify-content: center;
}

.v-qrcode-box .qrcode-image {
  width: 96px;
  height: 96px;
}

/* 便民服务 */
/** serice */
.v-card__body .v-flex-row {
  display: flex;
  width: 100%;
  flex-direction: row;
  justify-content: space-around;
  justify-items: center;
}
.v-icon-region {
  display: flex;
  flex: 1 1 33.3333%;
  flex-direction: column;
  justify-items: center;
  justify-content: center;
}
.v-icon-region .region-text {
  padding-top: 24rpx;
  justify-content: center;
  text-align: center;
  font-size: 28rpx;
}

.v-icon-region .v-line-inner .image-region {
  width: 100%;
  justify-content: center;
  justify-items: center;
}

.image-region {
  text-align: center;
  width: 43px;
  height: 43px;
}
.v-row-line {
  height: 12rpx;
  width: 100%;
  border-bottom: 1px solid rgba(240, 241, 243, 1);
}

.v-card__title > .v-title {
  font-size: 32rpx;
  font-weight: 500;
  line-height: 44rpx;
}

.v-flex-row {
  display: flex;
  flex-direction: row;
}
.body-inner-box {
  /* width: 100%; */
  display: flex;
  flex-direction: column;
  padding: 0px 32rpx;
}

/* header center */
.home-qrcode-warp {
  display: flex;
  flex-direction: row;
  -ms-flex-direction: row;
  flex: 1 1 100%;
  justify-self: center;
  justify-content: center;
  /* border: 1px solid #f6c330; */
  padding: 24rpx 24rpx 24rpx 24rpx;
}

.qrcode-box {
  display: flex;
  flex-direction: column;
  -ms-flex-direction: column;
  flex: 1 1 auto;
  justify-content: center;
  justify-items: center;
  /* border: 1px solid #f6c330; */
}

.home-qrcode-warp .did-box-bg {
  display: flex;
  flex: 1 1 100%;
  flex-direction: column;
  -ms-flex-direction: column;
  margin-top: -246rpx;
  justify-content: center;
  justify-items: center;
  min-height: 546rpx;
  padding-top: -100rpx;
  padding-bottom: 12rpx;
  /* border:1px solid #f6c330; */
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
.locked-icon-box {
  display: flex;
  flex: 1 1 100%;
  flex-direction: row;
  justify-content: center;
  justify-items: center;
  /* border: 1px solid #f6c330; */
  padding: 12px;
}
.locked-icon-box image {
  /* border: 1px solid #f6c330; */
  justify-self: center;
  width: 66px;
  height: 66px;
}

.qrcode-box .locked-desc {
  flex: 0 0 auto;
  text-align: center;
  font-weight: 600;
  color: #0c123d;
  line-height: 22px;
  padding: 6px 12px;
}

.did-box-bg .bottom-desc {
  flex: 0 0 auto;
  text-align: center;
  justify-self: center;
}

.qrcode-box--inner .qrcode-did {
  display: flex;
  flex: 1 1 auto;
  justify-items: center;
  justify-content: center;
  /* border: 1px solid #f6c330; */
}

.qrcode-show-box {
  position: relative;
  display: flex;
  flex-direction: row;
  flex: 1 1 auto;
  justify-content: center;
  justify-items: center;
  /* border: 1px solid #f6c330; */
}

.qrcode-show-box image {
  width: 400rpx;
  height: 400rpx;
  /* border: 1px solid #f6c330; */
}

.canvas-box {
  position: fixed;
  left: 0;
  top: 999999rpx;
}

/* Modal Styles */
.v-modal-mask {
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 9000;
  position: fixed;
  overflow: hidden;
  background: #000000;
  opacity: 0.4;
  color: #fff;
}

.v-modal-box {
  width: calc(100% - 80rpx);
  /* min-height: 376rpx; */
  z-index: 999999;
  position: fixed;
  background: transparent;
  display: flex;
  flex-direction: column;
  justify-items: center;
  justify-content: center;
  margin: auto;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
}
.v-modal-box .modal-inner {
  background: #fff;
  flex: 1 0 auto;
  border-radius: 40rpx;
  padding: 36rpx 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  justify-items: center;
}
.modal-inner .modal-title {
  text-align: center;
  flex: 0 0 auto;
  font-size: 36rpx;
  font-weight: 600;
  color: #0c123d;
  line-height: 50rpx;
}

.modal-inner > view:not(:last-child) {
  padding-bottom: 38rpx;
}

.modal-inner .v-modal-form {
  flex: 1 1 auto;
  display: flex;
  flex-direction: row;
  justify-items: center;
  justify-content: center;
  /* border: 1px solid #f5c95c; */
}

.v-modal-form .v-input {
  flex: 0 0 100%;
  /* border: 1px solid #f5c95d; */
  background: #f3f3f3;
  border-radius: 16rpx;
}

.modal-inner .v-modal-form input {
  /* flex: 1 0 100%; */
  height: 88rpx;
  /* border: 1rpx solid #ccc; */
  margin: 0rpx 24rpx;
  line-height: 88rpx;
}

.modal-inner .v-modal-form input::placeholder {
  margin-left: 24rpx;
  background: transparent;
}
