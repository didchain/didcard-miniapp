/* pages/creator/backup/backup.wxss */
.v-tips-icon {
  padding: 32rpx 0 16rpx 0;
}

.v-tips-ul {
  width: 100%;
  display: flex;
  flex-direction: column;
  -ms-flex-direction: column;
}

.v-tips-prefix text {
  color: #f6c330;
  font-size: 32rpx;
  font-weight: 600;
}

.v-tips-ul li {
  flex: 0 0 100%;
  line-height: 44rpx;
  font-size: 30rpx;
  padding-left: 16rpx;
  font-weight: 500;
}

/** footer */


.v-inner .vpx-24 l-button {
  padding: 24rpx auto;
}

.inner-skip {
  text-align: center;
  margin-top: 20rpx;
}

.inner-skip span {
  line-height: 44rpx;
  padding-bottom: 8rpx;
  border-bottom: 6rpx solid #f6c330;
}

.export-box-hide {
  position: fixed;
  z-index: -11;
}