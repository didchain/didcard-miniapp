/* pages/widgets/textrect-action/index.wxss */
.vtext-rect-action {
  width: 100%;
  display: flex;
  flex-direction: row;
}
.vtext-rect-action .vtext-rect {
  display: flex;
  flex: 1 0;
  flex-direction: column;
  margin-bottom: 24rpx;
}

.vtext-rect .vtext-rect-row {
  flex-direction: row;
}

.vtext-rect .vlabel label {
  font-size: 24rpx;
  font-weight: 400;
  color: #9b9c9f;
}
.vtext-rect .vtext {
  font-size: 30rpx;
  padding-top: 12rpx;
}

.vtext-rect .vtext > span {
  font-weight: 500;
  line-height: 42rpx;
}

.vtext-rect-action .vtext-tail-action {
  display: flex;
  flex-direction: column;
  flex: 0 1;
  align-items: flex-end;
}
.vtext-tail-action .v-action-inner {
  display: flex;
  flex-direction: column;
  -ms-flex-direction: column;
  -ms-flex-order: inherit;
  flex: 1 1;
  justify-content: center;
}
.vtext-tail-action .v-icon {
  width: 44rpx;
  height: 44rpx;
}

.v-icon .v-icon-circle {
  width: 100%;
  height: 100%;
  background-image: url(data:image/png;base64,);
  color: #fff;
  background-size: cover;
}
