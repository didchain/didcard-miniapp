.vbtn-bar {
  display: flex;
  flex: 1 1 100%;
  flex-direction: row;
  -ms-flex-direction: row;
  justify-content: space-between;
  justify-items: center;
  background: linear-gradient(90deg, #f5c95c 0%, #fad884 100%);
  border-radius: 8px;
}

.vbtn-bar .vbtn-bar--rect {
  flex: 1 1 auto;
  padding: 12rpx 12rpx;
}

.vbtn-bar .vbtn-action {
  flex: 0 1 auto;
  display: flex;
  justify-items: center;
  justify-content: center;
  padding: 16rpx 12rpx;
}

.vbtn-action .v-icon {
  flex: 0 1 auto;
  display: flex;
  justify-items: center;
  justify-content: center;
  justify-self: center;
}

.v-icon .v-icon-circle {
  height: 40rpx;
  width: 40rpx;
  justify-self: center;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
